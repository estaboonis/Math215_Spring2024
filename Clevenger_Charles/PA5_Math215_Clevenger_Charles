    {
      "cell_type": "code",
      "source": [
        "def compute_page_rank(filename, leap_prob=0.10, link_prob=0.90):\n",
        "    m = read_matrix_from_file(filename)\n",
        "\n",
        "    m[:, 23] = np.ones(len(m))\n",
        "\n",
        "    tm = compute_transition_matrix(m, leap_prob, link_prob)\n",
        "    pr = mix_markov_chain(tm, 6)[0]\n",
        "    return pr\n",
        "\n",
        "def plot_page_ranks(pr, top_n=50):\n",
        "    sorted_indices = np.argsort(pr)[::-1]\n",
        "    top_indices = sorted_indices[:top_n]\n",
        "    top_pr = pr[top_indices]\n",
        "\n",
        "    plt.figure(figsize=(12, 6))\n",
        "    plt.bar(range(len(top_pr)), top_pr)\n",
        "    plt.xlabel('Vertex')\n",
        "    plt.ylabel('Page Rank')\n",
        "    plt.title('Page Rank for Top 50 Vertices')\n",
        "    plt.xticks(range(len(top_pr)), top_indices)\n",
        "    plt.show()\n",
        "\n",
        "pr_medium23 = compute_page_rank('medium_graph.txt')\n",
        "plot_page_ranks(pr_medium23)\n"
      ],
